{
  "name": "admin-blogs",
  "version": "2.0.0",
  "private": true,
  "description": "admin for blogs",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "publish": "umi build && node kodo.js",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "playwright": "playwright install && playwright test",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.6.13",
    "@ant-design/pro-descriptions": "^1.10.46",
    "@ant-design/pro-form": "^1.55.0",
    "@ant-design/pro-layout": "^6.33.0",
    "@ant-design/pro-table": "^2.64.1",
    "@umijs/route-utils": "^2.0.5",
    "ahooks": "^3.1.10",
    "antd": "^4.24.13",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "^0.32.1",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.3.2",
    "rc-util": "^5.18.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dev-inspector": "^1.7.1",
    "react-dom": "^17.0.2",
    "react-easy-crop": "^4.0.2",
    "react-helmet-async": "^1.2.3",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.4.5",
    "umi": "^3.5.41"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@playwright/test": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/history": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@umijs/fabric": "^2.10.2",
    "@umijs/openapi": "^1.4.0",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/plugin-openapi": "^1.3.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.38",
    "@umijs/preset-react": "^2.1.1",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "co": "^4.6.0",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "express": "^4.17.3",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.3.4",
    "mockjs": "^1.1.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "prettier": "^2.5.1",
    "puppeteer-core": "^13.4.1",
    "qiniu": "^7.4.0",
    "rd": "^2.0.1",
    "stylelint": "^13.0.0",
    "swagger-ui-react": "^4.5.2",
    "typescript": "^4.6.2",
    "umi-serve": "^1.9.10",
    "webpack": "^5.88.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
